  <nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <targets>
      <target name="RollingLogFile" xsi:type="File" fileName=".\invoiceMe.log" archiveFileName=".\invoiceMe.{#}.log" archiveNumbering="Rolling" maxArchiveFiles="10" archiveAboveSize="10485760" layout="${pad:padding=-5:inner=${uppercase:${level}}} | ${processid} | ${threadname} | ${threadid} | ${date} | ${message} ${newline} ${exception:format=toString}" />
    </targets>
    <rules>
      <logger name="FileLogger" minlevel="Error" writeTo="RollingLogFile" />
    </rules>
  </nlog>

